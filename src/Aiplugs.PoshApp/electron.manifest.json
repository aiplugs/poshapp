{
  "executable": "Aiplugs.PoshApp",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": true,
  "build": {
    "appId": "com.aiplugs.poshapp",
    "productName": "POSH App",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "0.9.9",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "publish": [{
      "provider": "github",
      "owner": "aiplugs",
      "repo":  "poshapp"
    }],
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}