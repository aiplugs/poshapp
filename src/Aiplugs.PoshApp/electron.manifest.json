{
  "executable": "Aiplugs.PoshApp",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": true,
  "build": {
    "appId": "com.aiplugs.poshapp",
    "productName": "POSH App",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "0.9.21-alpha7",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "./bin/wwwroot/icon/poshapp-icon-256x256.ico"
    },
    "mac": {
      "icon": "./bin/wwwroot/icon/poshapp-icon-512x512.icns"
    },
    "linux": {
      "icon": "./bin/wwwroot/icon/"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "aiplugs",
        "repo": "poshapp"
      }
    ],
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}