    <partial name="PowershellUI/Progress" />
    <partial name="PowershellUI/Status" />
    <partial name="PowershellUI/Index" />
    <partial name="Settings/Nav" />
    <partial name="Settings/Activation" />
    <partial name="Settings/Version" />
    <partial name="Scripts/Nav" />
    <partial name="Scripts/Index" />
    <partial name="Repositories/Nav" />
    <partial name="Repositories/Index" />
    <partial name="Pages/Parts/ParametersForm" />
    <partial name="Pages/Parts/DataViewer" />
    <partial name="Pages/Nav" />
    <partial name="Pages/Detail" />
    <partial name="Pages/List" />
    <partial name="Pages/Singleton" />
    <partial name="App" />

    <script src="/lib/monaco-editor/main.bundle.js"></script>
    <script src="/js/main.js" type="module"></script>
</body>
</html>